  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "// Database Scripts": "// SQLite database management commands",
    "db:push": "drizzle-kit push",
    "db:init": "mkdir -p data && drizzle-kit push && tsx scripts/init-admin.ts",
    "db:backup": "tsx scripts/db-backup.ts",
    "db:restore": "tsx scripts/db-restore.ts",
    "db:reset": "tsx scripts/db-reset.ts",
    "db:verify": "tsx scripts/db-verify.ts",
    "// Admin Scripts": "// Admin user management commands",
    "admin:create": "tsx scripts/create-admin.ts",
    "// Testing Scripts": "// Development and testing utilities",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:upload": "jest tests/__tests__/upload",
    "test:offline": "NODE_ENV=test tsx server/index.ts --test-mode",
    "// Deployment Scripts": "// Create deployment packages",
    "deploy:export": "bash export-for-hosting.sh",
    "// Fly.io Scripts": "// Fly.io deployment and management commands",
    "fly:deploy": "flyctl deploy",
    "fly:logs": "flyctl logs",
    "fly:status": "flyctl status",
    "fly:ssh": "flyctl ssh console",
    "fly:open": "flyctl open",
    "fly:restart": "flyctl apps restart",
    "fly:backup": "flyctl ssh sftp get /data/database.db ./data/backups/flyio-backup-$(date +%Y%m%d-%H%M%S).db",
    "fly:volumes": "flyctl volumes list",
    "fly:certs": "flyctl certs show"
  },
